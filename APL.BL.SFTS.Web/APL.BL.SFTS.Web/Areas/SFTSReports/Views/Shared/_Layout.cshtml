<!DOCTYPE html>
<html lang="en">
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0, minimum-scale=1.0, maximum-scale=1.0, user-scalable=0" />
    <meta name="description" content="">
    <meta name="author" content="Administrator">
    <meta name="DirectoryKey" content="@System.Configuration.ConfigurationManager.AppSettings["DirectoryKey"].ToString()" />

    <title>SFTS | @ViewBag.Title</title>
    <link rel="icon" type="image/png" sizes="36x36" href="~/Content/img/favicon.png" />
    <!-- CSS-->
    <link href="~/Content/QA/reset.css" rel="stylesheet" />
    <link href="~/Content/QA/style.css" rel="stylesheet" />
    <link href="~/Content/w3.css" rel="stylesheet" />
    <link href="~/Content/bootstrap.min.css" rel="stylesheet" />
    <link href="~/Themes/assets/css/CustomStyles.css" rel="stylesheet" />

    <!-- Theme -->
    <link href="~/Themes/assets/css/main.css" rel="stylesheet" type="text/css" />
    <link href="~/Themes/assets/css/plugins.css" rel="stylesheet" type="text/css" />
    <link href="~/Themes/assets/css/responsive.css" rel="stylesheet" type="text/css" />
    <link href="~/Themes/assets/css/icons.css" rel="stylesheet" type="text/css" />
    <!-- Font Awesome -->
    <link href="~/bower_components/font-awesome/css/font-awesome.min.css" rel="stylesheet" />
    <!-- Ionicons -->
    <link href="~/bower_components/Ionicons/css/ionicons.min.css" rel="stylesheet" />
    <!-- Theme style -->
    <link href="~/dist/css/AdminLTE.min.css" rel="stylesheet" />
    <!-- AdminLTE Skins. Choose a skin from the css/skins
      folder instead of downloading all of them to reduce the load. -->

    <link href="~/dist/css/skins/_all-skins.min.css" rel="stylesheet" />
    <!-- Google Font -->
    <link rel="stylesheet" href="https://fonts.googleapis.com/css?family=Source+Sans+Pro:300,400,600,700,300italic,400italic,600italic" />

    <link href="~/Content/GoogleFont.css" rel="stylesheet" />
    <link href="~/Themes/plugins/datePicker/css/bootstrap-datepicker.min.css" rel="stylesheet" />

    <!--=== JavaScript ===-->
    <script type="text/javascript" src="~/Themes/assets/js/libs/jquery-1.10.2.min.js"></script>
    <script type="text/javascript" src="~/Themes/plugins/jquery-ui/jquery-ui-1.10.2.custom.min.js"></script>
    <script type="text/javascript" src="~/Themes/assets/js/libs/lodash.compat.min.js"></script>

    <!-- Smartphone Touch Events -->
    <script type="text/javascript" src="~/Themes/plugins/touchpunch/jquery.ui.touch-punch.min.js"></script>
    <script type="text/javascript" src="~/Themes/plugins/event.swipe/jquery.event.move.js"></script>
    <script type="text/javascript" src="~/Themes/plugins/event.swipe/jquery.event.swipe.js"></script>

    <!-- General -->
    <script type="text/javascript" src="~/Themes/assets/js/libs/breakpoints.js"></script>
    <script type="text/javascript" src="~/Themes/plugins/respond/respond.min.js"></script> <!-- Polyfill for min/max-width CSS3 Media Queries (only for IE8) -->
    <script type="text/javascript" src="~/Themes/plugins/cookie/jquery.cookie.min.js"></script>
    @*<script type="text/javascript" src="~/Themes/plugins/slimscroll/jquery.slimscroll.min.js"></script>
        <script type="text/javascript" src="~/Themes/plugins/slimscroll/jquery.slimscroll.horizontal.min.js"></script>*@

    <script type="text/javascript" src="~/Themes/plugins/sparkline/jquery.sparkline.min.js"></script>
    <script type="text/javascript" src="~/Themes/plugins/flot/jquery.flot.min.js"></script>
    <script type="text/javascript" src="~/Themes/plugins/flot/jquery.flot.tooltip.min.js"></script>
    <script type="text/javascript" src="~/Themes/plugins/flot/jquery.flot.resize.min.js"></script>
    <script type="text/javascript" src="~/Themes/plugins/flot/jquery.flot.time.min.js"></script>
    <script type="text/javascript" src="~/Themes/plugins/flot/jquery.flot.growraf.min.js"></script>
    <script type="text/javascript" src="~/Themes/plugins/easy-pie-chart/jquery.easy-pie-chart.min.js"></script>
    <script type="text/javascript" src="~/Themes/plugins/daterangepicker/moment.min.js"></script>
    <script type="text/javascript" src="~/Themes/plugins/daterangepicker/daterangepicker.js"></script>
    <script type="text/javascript" src="~/Themes/plugins/blockui/jquery.blockUI.min.js"></script>
    <script type="text/javascript" src="~/Themes/plugins/fullcalendar/fullcalendar.min.js"></script>
    <!-- Noty -->
    <script type="text/javascript" src="~/Themes/plugins/noty/jquery.noty.js"></script>
    <script type="text/javascript" src="~/Themes/plugins/noty/layouts/top.js"></script>
    <script type="text/javascript" src="~/Themes/plugins/noty/themes/default.js"></script>
    <!-- Forms -->
    <script type="text/javascript" src="~/Themes/plugins/uniform/jquery.uniform.min.js"></script>
    <script type="text/javascript" src="~/Themes/plugins/select2/select2.min.js"></script>
    <!-- App -->
    <script type="text/javascript" src="~/Themes/assets/js/app.js"></script>
    <script type="text/javascript" src="~/Themes/assets/js/plugins.js"></script>
    <script type="text/javascript" src="~/Themes/assets/js/plugins.form-components.js"></script>
    <link href="~/Content/tabs.css" rel="stylesheet" />
    <!-- DatePicker -->
    <script src="~/Themes/plugins/datePicker/js/bootstrap-datepicker.min.js"></script>
    <script>
        var DirectoryKey = "";
        $(document).ready(function () {
            DirectoryKey = angular.element(document).find('meta[name=DirectoryKey]').prop("content");
        });
    </script>

    <script type="text/javascript">
        $(function () {
            $(".datepicker").datepicker({
                format: "dd/mm/yyyy",
                todayHighlight: true,
                autoclose: true
            });

            $(".datepicker").datepicker('setDate', new Date());
        });

    </script>
    <script>
        $(document).ready(function () {
            "use strict";
            $('.select2-select-00').select2({
                allowClear: true
            });

            toastr.options = {
                "closeButton": true,
                "debug": false,
                "newestOnTop": false,
                "progressBar": false,
                "positionClass": "toast-bottom-right",
                "preventDuplicates": false,
                "onclick": null,
                "showDuration": "100",
                "hideDuration": "1000",
                "timeOut": "5000",
                "extendedTimeOut": "1000",
                "showEasing": "swing",
                "hideEasing": "linear",
                "showMethod": "fadeIn",
                "hideMethod": "fadeOut"
            }

            App.init(); // Init layout and core plugins
            Plugins.init(); // Init all plugins
            FormComponents.init(); // Init all form-specific plugins
        });
    </script>
    <!-- Demo JS -->
    <script type="text/javascript" src="~/Themes/assets/js/custom.js"></script>
    <script src="~/Themes/assets/js/demo/ui_general.js"></script>

    <!-- Angular TimePicker -->
    <link href="~/Content/ngTimepicker.css" rel="stylesheet" />

    <!--link href="../Content/customizeBL.css" rel="stylesheet" /-->

</head>

<body class="ng-cloak hold-transition sidebar-mini skin-yellow-light" ng-app="SFTS_Report">
    <div>
        <!-- Site wrapper -->
        <div class="wrapper">
            <div ng-controller="tokenCtrl">
                <div ng-controller="MenuCtlr">
                    <div>
                        <!--Main Header -->
                        @Html.Partial("~/Areas/SFTS/Views/Shared/_Partial/_MainHeader.cshtml")
                    </div>

                    <!-- =============================================== -->

                    <div>
                        <!--Left Side Bar -->
                        @Html.Partial("~/Areas/SFTS/Views/Shared/_Partial/_LeftSideBar.cshtml")
                    </div>
                </div>
            </div>



            <!-- =============================================== -->
            <!-- Content Wrapper. Contains page content -->
            <div class="content-wrapper">
                <!-- Main content -->
                <div style="padding-left: 5px; padding-right: 5px;">
                    @RenderBody()
                </div>
                <!-- /.box-body -->
                <!-- /.content -->
            </div>
            <!-- /.content-wrapper -->

            <footer class="main-footer">
                <div class="pull-right hidden-xs">
                    <b>Version</b> 2.4.0
                </div>
                <strong>Copyright © @DateTime.Now.Year Arena Phone (BD) Ltd</a>.</strong> All rights
                reserved.
            </footer>


        </div>
        <!-- ./wrapper -->
        <!-- jQuery 3 -->
        @*<script src="~/bower_components/jquery/dist/jquery.min.js"></script>*@
        <!-- Bootstrap 3.3.7 -->
        <script src="~/bower_components/bootstrap/dist/js/bootstrap.min.js"></script>
        <!-- SlimScroll -->
        <!--<script src="~/bower_components/jquery-slimscroll/jquery.slimscroll.min.js"></script>-->
        <!-- FastClick -->
        <script src="~/bower_components/fastclick/lib/fastclick.js"></script>
        <!-- AdminLTE App -->
        <script src="~/dist/js/adminlte.min.js"></script>
        <!-- AdminLTE for demo purposes -->
        <script src="~/dist/js/demo.js"></script>
        <script>
            $(document).ready(function () {
                $('.sidebar-menu').tree()
            })
        </script>


        <script src="~/Scripts/angular.min.js"></script>
        <script src="~/Scripts/ng-map.js"></script>
        <script src="~/Scripts/angular-route.min.js"></script>
        <script src="~/Scripts/ui-grid.min.js"></script>
        <script src="~/Scripts/angular-touch.min.js"></script>
        <script src="~/Scripts/angular-animate.min.js"></script>
        <link href="~/Content/ui-grid.min.css" rel="stylesheet" />

        <!-- Export Data Refferences -->
        <script src="~/Scripts/export-ui-grid/csv.js"></script>
        <script src="~/Scripts/export-ui-grid/pdfmake.js"></script>
        <script src="~/Scripts/export-ui-grid/vfs_fonts.js"></script>

        <script src="~/Areas/SFTSReports/ScriptsNg/Modules/SFTS_Report.js"></script>

        <!-- Dynamic Menu Refferences -->
        <script src="~/ScriptsNg/Controllers/LogoutCtrl.js"></script>
        <script src="~/Areas/SFTS/ScriptsNg/Service/MenuService.js"></script>
        <script src="~/Areas/SFTS/ScriptsNg/Controllers/MenuCtlr.js"></script>
        <script src="~/Areas/Token/TokenService.js"></script>


        @RenderSection("AngularScript", false)

        <!-- Dynamic Message Refferences -->
        <script src="~/Scripts/toastr.min.js"></script>
        <link href="~/Content/toastr.min.css" rel="stylesheet" />
        <script src="~/Scripts/CustomToastrMessage.js"></script>

        <!-- /Use LocalStroge Session Refferences -->
        @*  <script src="https://cdnjs.cloudflare.com/ajax/libs/ngStorage/0.3.10/ngStorage.js"></script>*@
        <script src="~/Scripts/ngStorage.js"></script>

        <script src="~/Scripts/jquery.signalR-2.2.0.js"></script>
        @*<script src="~/signalr/hubs"></script>*@

        @*HashEncode*@
        <script src="~/Scripts/hmac-sha256.js"></script>
        <script src="~/Scripts/enc-base64-min.js"></script>

        @*For Date time picker in table*@
        <link href="~/Content/bootstrap-theme.css" rel="stylesheet" />
        <link href="~/Content/font-awesome.css" rel="stylesheet" />
        <script src="~/Scripts/ui-bootstrap-0.7.0.js"></script>
        <script src="~/Scripts/ui-bootstrap-tpls-0.7.0.js"></script>

    </div>
</body>
</html>

