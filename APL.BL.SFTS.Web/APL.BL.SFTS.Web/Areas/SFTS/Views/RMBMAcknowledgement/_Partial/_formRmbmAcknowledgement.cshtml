
<form novalidate name="frmAmbmShopVisit" id="frmAmbmShopVisit" class="form-horizontal row-border">
    <input type="hidden" id="hVisitId" readonly="readonly" ng-model="hVisitId" />

    <div class="row">
        <div class="col-md-6">
            <div style="height: 10px;"></div>

            <div class="form-group  m-b-10 clearfix">
                <label class="col-md-4 control-label" for="input17">Shop Code :</label>
                <div class="col-md-6">
                    <label class="control-label">{{ShopCode}}</label>
                </div>
            </div>   

            <div class="form-group m-b-10 clearfix">
                <label class="col-md-4 control-label" for="input17">Shop Name :</label>
                <div class="col-md-6">
                    <label class="control-label">{{ShopName}}</label>
                </div>
            </div>   

           
            
            <div class="form-group m-b-10 clearfix">
                <label class="col-md-4 control-label" for="input17">Comments :</label>
                <div class="col-md-6">
                    <textarea rows="2" cols="3"
                              id="txtComments"
                              @*ng-disabled="IsCommentDisabled"*@
                              class="form-control"
                              name="txtComments" ng-model="Comments"></textarea>
                </div>
            </div>
             
            <div style="height: 10px;"></div> 

            @*<div class="form-group m-b-10 clearfix">
                <label class="control-label" style="color: red;" ng-show="IsMapNotAvail">{{msgMapNotAvail}}</label>
            </div>*@

        </div>
        <div class="col-md-6">
            <div style="height: 10px;"></div>

            <div class="form-group m-b-10 clearfix">
                <label class="col-md-4 control-label" for="input17">Shop Address :</label>
                <div class="col-md-6">
                    <label class="control-label">{{ShopAddress}}</label>
                </div>
            </div>

            <div class="form-group m-b-10 clearfix">
                <label class="col-md-4 control-label" for="input17">Visited By :</label>
                <div class="col-md-6">
                    <label class="control-label">{{VisitedBy}}</label>
                </div>
            </div>

            <div class="form-group m-b-10 clearfix">
                <label class="col-md-4 control-label" for="input17">Visited Date :</label>
                <div class="col-md-6">
                    <label class="control-label">{{VisitedDate}}</label>
                </div>
            </div>
  
            <div style="height: 10px;"></div>
        </div>

        
    </div>
</form>




