
<form novalidate name="frmMarketUpdateSearch" id="frmMarketUpdateSearch" class="form-horizontal row-border">

    <div class="row">

        <div class="col-md-4">
            <div style="height: 10px;"></div>

            <div class="form-group">
                <label class="col-md-4 control-label">Date From</label>
                <div class="col-md-7">
                    <input type="text"
                           id="txtDateFrom"
                           class="form-control datepicker"
                           name="txtDateFrom" ng-model="DateFrom" placeholder="dd-MM-yyyy" required />
                </div>
            </div>

            <div style="height: 10px;"></div>
        </div>

        <div class="col-md-4">
            <div style="height: 10px;"></div>

            <div class="form-group">
                <label class="col-md-4 control-label">Date To</label>
                <div class="col-md-7">
                    <input type="text"
                           id="txtDateTo"
                           class="form-control datepicker"
                           name="txtDateTo" ng-model="DateTo" placeholder="dd-MM-yyyy" required />
                </div>
            </div>

            <div style="height: 10px;"></div>
        </div>

        <div class="col-md-4">
            <div style="height: 10px;"></div>

            <span id="Show" class="btn btn-info margin-right-btn" ng-click="GetMarketUpdateSearch()">
                <i class="icon-search"></i> Search
            </span>
            <div style="height: 10px;"></div>

        </div>


    </div>
</form>





