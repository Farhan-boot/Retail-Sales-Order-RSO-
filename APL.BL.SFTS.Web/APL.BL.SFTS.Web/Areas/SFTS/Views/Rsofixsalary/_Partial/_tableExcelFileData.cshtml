
<div style="height: 20px;"></div>
<label style="color:red; font-weight: bold">Total Record(s): {{TotalRowCount}} </label><label style="margin-left:100px; color:red; font-weight: bold">Total Salary amount: {{TotalTargetValue}} Tk</label>
<table class="table table-striped table-fixed">
    <thead style="background-color: #ecf0f5">
        <tr>
            <th>ROSCODE</th>
            <th>SALAY YEAR MONTH</th>
            <th>BANK NAME</th>
            <th>BANK AC</th>
            <th>DOB</th>
            <th>JOIN DATE</th>
            <th>WORKING DAY</th>
            <th>TOTAL SALARY</th>
            <th>VENDOR</th>
            <th>STATUS</th>
        </tr>
    </thead>
    <tbody>
        <tr ng-repeat="excelData in uploadedExcelData">
            <td>{{excelData.ROSCODE}}</td>
            <td>{{excelData.SALAY_YEAR_MONTH}}</td>
            <td>{{excelData.BANK_NAME}}</td>
            <td>{{excelData.BANK_AC}}</td>
            <td>{{excelData.DOB}}</td>
            <td>{{excelData.JOIN_DATE}}</td>
            <td>{{excelData.WORKING_DAY}}</td>
            <td>{{excelData.TOTAL_SALARY}}</td>
            <td>{{excelData.VENDOR}}</td>
            <td>{{excelData.STATUS}}</td>
        </tr>
    </tbody>
</table>

