
<form novalidate name="frmTradeMaterials" id="frmTradeMaterials" class="form-horizontal row-border">
    <input type="hidden" id="hTradeMaterialId" readonly="readonly" ng-model="hTradeMaterialId" />
    <div class="row">
        <div class="col-md-6">
            <div style="height: 10px;"></div>

            <div class="form-group  m-b-10 clearfix">
                <label class="col-md-4 control-label" for="input17">Code :</label>
                <div class="col-md-6">
                    <input type="text"
                           id="txtTradeMaterialCode"
                           class="form-control"
                           ng-model="TradeMaterialCode"
                           name="txtTradeMaterialCode" required />
                </div>
                <span class="error">*</span>
            </div>   

            <div class="form-group  m-b-10 clearfix">
                <label class="col-md-4 control-label" for="input17">Name :</label>
                <div class="col-md-6">
                    <input type="text"
                           id="txtTradeMaterialName"
                           class="form-control"
                           ng-model="TradeMaterialName"
                           name="txtTradeMaterialName" required/> 
                </div>
                <span class="error">*</span>
            </div>   

            <div class="form-group m-b-10 clearfix">
                <label class="col-md-4 control-label" for="input17">Is Active :</label>
                <div class="col-md-6">
                    <input type="checkbox" ng-model="IsActive" id="chkIsActive" name="chkIsActive" />
                </div>
            </div>

        </div>

     </div>
</form>

