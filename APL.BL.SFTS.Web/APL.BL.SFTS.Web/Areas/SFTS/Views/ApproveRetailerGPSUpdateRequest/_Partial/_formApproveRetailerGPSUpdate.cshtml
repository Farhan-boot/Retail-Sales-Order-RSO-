
<form novalidate name="frmApproveRetailerCreation" id="frmApproveRetailerCreation" class="form-horizontal row-border">
    <input type="hidden" id="hRetailerModifyId" readonly="readonly" ng-model="hRetailerModifyId" />

    <div class="row">
        <div class="col-md-6">
            <div style="height: 10px;"></div>

            <div class="form-group  m-b-10 clearfix">
                <label class="col-md-4 control-label" for="input17">Retailer Code :</label>
                <div class="col-md-6">
                    <label class="control-label">{{RetailerCode}}</label>
                </div>
            </div>   

            <div class="form-group m-b-10 clearfix">
                <label class="col-md-4 control-label" for="input17">Distributor :</label>
                <div class="col-md-6">
                    <label class="control-label">{{Distributor}}</label>
                </div>
            </div>   

            <div class="form-group m-b-10 clearfix">
                <label class="col-md-4 control-label" for="input17">Retailer Name :</label>
                <div class="col-md-6">
                    <label class="control-label">{{RetailerName}}</label>
                </div>
            </div>   

            @*<div class="form-group m-b-10 clearfix">
                <label class="col-md-4 control-label" for="input17">Address :</label>
                <div class="col-md-6">
                    <label class="control-label">{{Address}}</label>
                </div>
            </div>*@   
            
            <div class="form-group m-b-10 clearfix">
                <label class="col-md-4 control-label" for="input17">Requester Comments :</label>
                <div class="col-md-6">
                    <label class="control-label">{{RequesterComments}}</label>
                </div>
            </div> 

            <div class="form-group m-b-10 clearfix">
                <label class="col-md-4 control-label" for="input17">Requested Date :</label>
                <div class="col-md-6">
                    <label class="control-label">{{RequestDate}}</label>
                </div>
            </div>

            <!-- <div class="form-group m-b-10 clearfix">
                <label class="col-md-4 control-label" for="input17">Approver Comments :</label>
                <div class="col-md-6">
                    <textarea rows="2" cols="3"
                              id="txtComments"
                              @*ng-disabled="IsCommentDisabled"*@
                              class="form-control"
                              name="txtComments" ng-model="ApproverComments"></textarea>
                </div>
            </div>-->
           
             
            <div style="height: 10px;"></div> 

            <div class="form-group m-b-10 clearfix">
                <label class="control-label" style="color: red;" ng-show="IsMapNotAvail">{{msgMapNotAvail}}</label>
            </div>

        </div>
        <div class="col-md-6">
            <div style="height: 10px;"></div>

            <div class="form-group m-b-10 clearfix">
                <label class="col-md-4 control-label" for="input17">Old Latitude :</label>
                <div class="col-md-6">
                    <label class="control-label">{{OldlatValue}}</label>
                </div>
            </div>

            <div class="form-group m-b-10 clearfix">
                <label class="col-md-4 control-label" for="input17">New Latitude :</label>
                <div class="col-md-6">
                    <label class="control-label">{{NewlatValue}}</label>
                </div>
            </div>

            <div class="form-group m-b-10 clearfix">
                <label class="col-md-4 control-label" for="input17">Old Longitude :</label>
                <div class="col-md-6">
                    <label class="control-label">{{OldlongValue}}</label>
                </div>
            </div>

            <div class="form-group m-b-10 clearfix">
                <label class="col-md-4 control-label" for="input17">New Longitude :</label>
                <div class="col-md-6">
                    <label class="control-label">{{NewlongValue}}</label>
                </div>
            </div>          



            <div style="height: 10px;"></div>
        </div>

        
    </div>
</form>




