
<form novalidate name="frmMarketUpdateType" id="frmMarketUpdateType" class="form-horizontal row-border">
    <input type="hidden" id="hMarketUpdateTypeId" readonly="readonly" ng-model="hMarketUpdateTypeId" />
    <div class="row">
        <div class="col-md-7">
            <div style="height: 10px;"></div>

            <div class="form-group  m-b-10 clearfix">
                <label class="col-md-4 control-label" for="input17">Type Name :</label>
                <div class="col-md-6">
                    <input type="text"
                           id="txtTypeName"
                           class="form-control"
                           ng-model="TypeName"
                           name="txtTypeName" required/> 
                </div>
                <span class="error">*</span>
              </div>     
 
                <div class="form-group m-b-10 clearfix">
                    <label class="col-md-4 control-label" for="input17">Is Active :</label>
                    <div class="col-md-6">
                        <input type="checkbox" ng-model="IsActive" id="chkIsActive" name="chkIsActive" />
                    </div>
                </div>

        </div>
    </div>
</form>

