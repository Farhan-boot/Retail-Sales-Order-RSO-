

<form novalidate name="frmEVLimits" id="frmEVLimits" class="form-horizontal row-border">
    <input type="hidden" id="hEVLImitId" readonly="readonly" ng-model="hEVLimitId" />
    <div class="row">
        <div class="col-md-6">
            <div style="height: 10px;"></div>

            <div class="form-group  m-b-10 clearfix">
                <label class="col-md-5 control-label" for="input17">MINIMUM TRANSFER AMOUNT :</label>
                <div class="col-md-5">
                    <input type="text"
                           id="txtMinAmount"
                           class="form-control numberonly"
                           ng-model="MinAmount"
                           name="txtMinAmount" required />
                </div>
                <span class="error">*</span>
            </div>

            <div class="form-group  m-b-10 clearfix">
                <label class="col-md-5 control-label" for="input17">MAXIMUM TRANSFER AMOUNT :</label>
                <div class="col-md-5">
                    <input type="text"
                           id="txtMaxAmount"
                           class="form-control numberonly"
                           ng-model="MaxAmount"
                           name="txtMaxAmount" required value="700" />
                </div>
                <span class="error">*</span>
            </div>

           
        </div>

    </div>
</form>

