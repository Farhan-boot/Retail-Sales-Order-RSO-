@model IEnumerable<APL.BL.SFTS.DataBridgeZone.SP_GET_FF_RSO_EARNING_INCENTIVE_FILE>

<div style="height: 20px;"></div>

@if (Model != null)
{
    <label style="color:red; font-weight: bold">Total Record(s):  @Model.First().ROW_COUNT</label><label style="color:red; font-weight: bold">, Total Target Value:  @Model.First().TOTAL_SALARY_VALUE</label>
    <input id="uploadCode" hidden="hidden" value="@Model.First().ID" />
                                <input id="hTargetId" hidden="hidden" ng-model="hTargetId" />
                                <input id="hVersion" hidden="hidden" ng-model="hVersion" />
                                <table class="table table-striped table-fixed">
                                    <thead style="background-color: #ecf0f5">
                                        <tr>
                                            <th>ROSCODE</th>
                                            <th>SALAY YEAR MONTH</th>
                                            <th>TYPE</th>                                            
                                            <th>AMOUNT</th>
                                            <th>REMARKS</th>
                                            
                                        </tr>
                                    </thead>
                                    <tbody>
                                        @foreach (var item in Model)
                                        {
                                            <tr>
                                                <td>@item.RSOCODE </td>
                                                <td>@item.BASE_YEAR_MONTH </td>
                                                <td>@item.TYPE </td>
                                                <td>@item.INCENTIVE </td>                                                
                                                <td>@item.REMARKS</td>
                                            </tr>
}
                                    </tbody>
                                </table>
}
