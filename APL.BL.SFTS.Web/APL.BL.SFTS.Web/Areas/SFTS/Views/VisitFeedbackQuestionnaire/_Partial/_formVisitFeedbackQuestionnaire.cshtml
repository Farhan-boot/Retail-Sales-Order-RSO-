
<form novalidate name="frmVisitFeedbackQuestionnaire" id="frmVisitFeedbackQuestionnaire" class="form-horizontal row-border">
    <input type="hidden" id="hQuestionnaireId" readonly="readonly" ng-model="hQuestionnaireId" />
    <div class="row">
        <div class="col-md-7">
            <div style="height: 10px;"></div>
            @*<div class="form-group m-b-10 clearfix">
                <label class="col-md-4 control-label">Questionnaire ID</label>
                <div class="col-md-6">

                    <input type="text"
                           id="txtQuestionnaireId"
                           class="form-control"
                           style="width: 50px"
                           readonly
                           name="txtQuestionnaireId"/> 
                    </div>
              </div>*@

            <div class="form-group  m-b-10 clearfix">
                <label class="col-md-4 control-label" for="input17">Visit Type</label>
                <div class="col-md-6">
                    <select id="ddlVisitType" class="select2-select-00"
                            ng-model="VisitType"
                            name="ddlVisitType"
                            data-placeholder="--Select Visit Type--"
                            style="width: 100%" ng-options="vst.VISIT_TYPE_ID as vst.VISIT_TYPE_NAME for vst in listVisitType" required>
                        <option></option>
                    </select>
                </div>
                <span class="error">*</span>
            </div>   

            <div class="form-group m-b-10 clearfix">
                <label class="col-md-4 control-label" for="input17">Entity Type</label>
                <div class="col-md-6">
                    <select id="ddlEntityType" class="select2-select-00"
                            ng-model="EntityType"
                            name="ddlEntityType"
                            data-placeholder="--Select Entity Type--"
                            style="width: 100%" required>
                        <option></option>
                        <option value="1">CUSTOMER VISIT</option>
                        <option value="2">SHOP VISIT</option>
                        <option value="3">RETAILER VISIT</option>
                        <option value="4">EVENT</option>
                        <option value="5">SITE</option>
                    </select>
                </div>
                <span class="error">*</span>
            </div>   

            <div class="form-group m-b-10 clearfix">
                <label class="col-md-4 control-label" for="input17">Center Type</label>
                <div class="col-md-6">
                    <select id="ddlCenterType" class="select2-select-00"
                            ng-model="CenterType"
                            name="ddlCenterType"
                            data-placeholder="--Select Center Type--"
                            style="width: 100%" ng-options="cntr.CENTER_TYPE_ID as cntr.CENTER_TYPE_NAME for cntr in listCenterType" required>
                        <option></option>
                    </select>
                </div>
                <span class="error">*</span>
            </div>   

                <div class="form-group m-b-10 clearfix">
                    <label class="col-md-4 control-label" for="input17">Is Active</label>
                    <div class="col-md-6">
                        <input type="checkbox" ng-model="IsActive" id="chkIsActive" name="chkIsActive" />
                    </div>
                </div>

        </div>


    </div>
</form>

